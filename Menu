    private void btnMhsActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        
        //remove panel
        mainPanel.removeAll();
        mainPanel.repaint();
        mainPanel.revalidate();
        
        //add panel
        mainPanel.add(MhsPanel);
        mainPanel.repaint();
        mainPanel.revalidate();

    }                                      

    private void btnDsnActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        
        //remove panel
        mainPanel.removeAll();
        mainPanel.repaint();
        mainPanel.revalidate();
        
        //add panel
        mainPanel.add(DsnPanel);
        mainPanel.repaint();
        mainPanel.revalidate();

    }                                      

    private void btnHomeActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:

        //remove panel
        mainPanel.removeAll();
        mainPanel.repaint();
        mainPanel.revalidate();

        //add panel
        mainPanel.add(HomePanel);
        mainPanel.repaint();
        mainPanel.revalidate();
    }                                       

    private void btnAdmActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        //remove panel
        mainPanel.removeAll();
        mainPanel.repaint();
        mainPanel.revalidate();

        //add panel
        mainPanel.add(AdmPanel);
        mainPanel.repaint();
        mainPanel.revalidate();
    }                                      

    private void btnLgtActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        
        int dialogBtn = JOptionPane.YES_NO_OPTION;
        int dialogResult = JOptionPane.showConfirmDialog(this,"Anda Yakin Akan Keluar?","PERINGATAN", dialogBtn);
        
        if(dialogResult == 0){
            //true condition
            System.exit(0);
        }else{
            //false condition 
        }
    }                                      

